                        </DockPanel>
                        <DockPanel>
                            <Label Content="Enable" Width="130" />
                            <TextBox Name ="PagePropertyEnable" Height="22" TextWrapping="Wrap" Text="" Width="162" DockPanel.Dock="Right" KeyUp="PagePropertyEnable_KeyUp" />
                        </DockPanel>
                    </StackPanel>
                </DockPanel>



