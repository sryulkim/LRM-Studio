                        <Separator Height="5" Margin="10"/>

                        <DockPanel>
                            <Label Content="Page Property" FontSize="13"  />
                        </DockPanel>
                        <DockPanel>
                            <Label Content="Name" Width="130" />
                            <TextBox Name ="PagePropertyName" Height="22" TextWrapping="Wrap" Text="" Width="162" DockPanel.Dock="Right" KeyUp="PagePropertyName_KeyUp" />
                        </DockPanel>
                        <DockPanel>
                            <Label Content="Width" Width="130" />
                            <TextBox Name ="PagePropertyWidth" Height="22" TextWrapping="Wrap" Text="" Width="162" DockPanel.Dock="Right" TextChanged="PagePropertyWidth_TextChanged" KeyUp="PagePropertyWidth_KeyUp" />
                        </DockPanel>
                        <DockPanel>
                            <Label Content="Height" Width="130" />
